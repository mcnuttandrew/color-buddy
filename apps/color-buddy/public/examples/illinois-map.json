{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "width": 250,
  "height": 250,
  "autosize": "none",
  "data": [
    {
      "name": "counties",
      "url": "data/us-10m.json",
      "format": {
        "type": "topojson",
        "feature": "counties"
      },
      "transform": [
        {
          "type": "formula",
          "expr": "toNumber(datum.id)",
          "as": "id2"
        },
        {
          "type": "filter",
          "expr": "datum.id2 > 17000 && datum.id2 < 18000"
        }
      ]
    }
  ],
  "projections": [
    {
      "name": "projection",
      "type": "albersUsa",
      "translate": [-100, 180],
      "scale": 2500
    }
  ],
  "scales": [
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "counties",
        "field": "id2"
      },
      "range": "category"
    }
  ],
  "marks": [
    {
      "type": "shape",
      "from": {
        "data": "counties"
      },
      "encode": {
        "update": {
          "fill": {
            "scale": "color",
            "field": "id2"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      },
      "transform": [
        {
          "type": "geoshape",
          "projection": "projection"
        }
      ]
    }
  ]
}
