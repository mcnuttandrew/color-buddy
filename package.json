{
  "name": "color-pal-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "types": "tsc --noEmit --watch",
    "prep-data": "cp node_modules/vega-datasets/data/* public/data/",
    "build-data": "mkdir data && cp node_modules/vega-datasets/data/* ./data/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "gen-schema": "npx ts-json-schema-generator --path src/lib/lint-language/lint-type.ts --type LintProgram > public/lint-schema.json --minify && vite-node scripts/pretty-schema.ts"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/chroma-js": "^2.4.3",
    "@types/color-namer": "^1.3.3",
    "@types/d3": "^7.4.3",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/web-worker": "^1.2.2",
    "autoprefixer": "^10.4.16",
    "jsdom": "^24.0.0",
    "netlify-cli": "^17.10.1",
    "postcss": "^8.4.32",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "ts-json-schema-generator": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vega-typings": "^1.0.1",
    "vite": "^5.0.0",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.1",
    "chroma-js": "^2.4.2",
    "color-namer": "^1.4.0",
    "colorjs.io": "^0.5.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "driver.js": "^1.3.1",
    "fracturedjsonjs": "^3.1.0",
    "idb-keyval": "^6.2.1",
    "jsonc-parser": "^3.2.1",
    "monaco-editor": "^0.45.0",
    "openai": "^4.21.0",
    "svelte-dnd-action": "^0.9.31",
    "svelte-portal": "^2.2.0",
    "tailwindcss": "^3.3.5",
    "vega": "^5.26.1",
    "vega-datasets": "^2.7.0",
    "vega-embed": "^6.23.0",
    "vega-lite": "^5.16.3"
  }
}
