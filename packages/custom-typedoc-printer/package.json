{
  "name": "@color-buddy/custom-typedoc-printer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.ts",
  "scripts": {
    "update-pal-lint": "vite-node main.ts ../palette-line/",
    "update-pal": "vite-node main.ts ../palette/",
    "update-color-namer": "vite-node main.ts ../color-namer/",
    "update-all": "npm run update-pal && npm run update-pal-lint && npm run update-color-namer"
  },
  "dependencies": {
    "color-namer": "^1.4.0",
    "vite": "^5.3.2",
    "vite-node": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/prettier": "^3.0.0",
    "typedoc": "^0.26.3",
    "typedoc-plugin-markdown": "^4.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
